@REM !!!!!!!!home.bat
@REM ********************************************************************************************************************************home.bat
@ECHO OFF
REM ================================================================================================================================home.bat
IF "%1"=="" GOTO Root
IF "%1"=="." GOTO GCD
IF /I "%1"=="MouseChsh" GOTO MouseChsh
IF "%1"=="*" GOTO SETCD
IF EXIST "%MouseChshZRL_Root%.%1\C" (
	%MouseChshZRL_Root:~0,2%
	CD "%MouseChshZRL_Root%.%1\C"
) ELSE (
	GOTO Root
)
GOTO END
REM --------------------------------------------------------------------------------------------------------------------------------home.bat
:MouseChsh
%MouseChshZRL_Root:~0,2%
CD "%MouseChshZRL_Root%\C\Project\src"
GOTO END
REM --------------------------------------------------------------------------------------------------------------------------------home.bat
:Root
%MouseChshZRL_Root:~0,2%
CD "%MouseChshZRL_Root%\C"
GOTO END
REM --------------------------------------------------------------------------------------------------------------------------------home.bat
:SETCD
SET MouseChshZRL_CD=%CD%
GOTO GCD
REM --------------------------------------------------------------------------------------------------------------------------------home.bat
:GCD
%MouseChshZRL_CD:~0,2%
CD "%MouseChshZRL_CD%"
GOTO END
REM ================================================================================================================================home.bat
:END
